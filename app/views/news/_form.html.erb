<%= semantic_form_for @news, :html => { :multipart => true } do |f| %>

<%f.inputs do%>
<%= f.input :title %>
<%= f.input :subtitle %>
<%= f.input :news_category, :include_blank => false%>
<%= f.input :by %>
<%= f.input :caption %>
<%= f.input :text %>
<%= f.input :metadata %>
<%= f.input :date ,:start_year => 2010%>

<%= f.file_field :news%>
<% end %>

<%= f.buttons %>



<% end %>